# Production Environment Variables
NODE_ENV=production
PORT=5001

# Database
DATABASE_URL="file:./production.db"

# JWT
JWT_SECRET="changez-ce-secret-par-quelque-chose-de-tres-securise"
JWT_EXPIRES_IN="30d"

# URLs
FRONTEND_URL=https://front_alice.alicebot.online
BACKEND_URL=https://back_alice.alicebot.online

# Email (à configurer avec votre service d'email)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=votre-email@gmail.com
SMTP_PASS=votre-mot-de-passe
EMAIL_FROM=noreply@alicebot.online

# Push Notifications (générer avec npx web-push generate-vapid-keys)
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
VAPID_SUBJECT=mailto:admin@alicebot.online

# Autres configurations
THROTTLE_TTL=60
THROTTLE_LIMIT=10
